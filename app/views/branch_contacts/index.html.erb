<h1>Listing branch_contacts</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Contacttype</th>
      <th>Contact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @branch_contacts.each do |branch_contact| %>
      <tr>
        <td><%= branch_contact.branch %></td>
        <td><%= branch_contact.contacttype %></td>
        <td><%= branch_contact.contact %></td>
        <td><%= link_to 'Show', branch_contact %></td>
        <td><%= link_to 'Edit', edit_branch_contact_path(branch_contact) %></td>
        <td><%= link_to 'Destroy', branch_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Branch contact', new_branch_contact_path %>
